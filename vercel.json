{
    "version": 2,
    "functions": {
        "app.js": {
            "includeFiles": [
                "views/**",
                "public/**",
                "users.json",
                "db.js",
                "borrowReturn.js"
            ]
        }
    },
    "builds": [
        {
            "src": "app.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app.js"
        }
    ]
}